{
  "name": "@yelloxing/debugger",
  "version": "1.0.10",
  "description": "🎈 一个用于移动端的WEB调试工具。",
  "main": "build/debug.console.min.js",
  "scripts": {
    "dev": "node_modules/.bin/webpack-dev-server --config webpack.dev.js",
    "clean": "cuf -d ./dist",
    "release": "npm run clean && npm run prod && npm run add-banner",
    "add-banner": "cuf --config ./cuf.config.js",
    "prod": "node_modules/.bin/webpack --config webpack.prod.js",
    "start": "npm install && npm run dev",
    "server": "cuf -s 20000"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yelloxing/debug.console.js.git"
  },
  "keywords": [
    "debugger",
    "console"
  ],
  "author": "心叶",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yelloxing/debug.console.js/issues"
  },
  "homepage": "https://github.com/yelloxing/debug.console.js#readme",
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/preset-env": "^7.10.2",
    "autoprefixer": "^9.7.1",
    "babel-loader": "^8.1.0",
    "css-loader": "^2.1.1",
    "cuf": "^1.2.0",
    "file-loader": "^4.2.0",
    "icrush-loader": "^0.2.1",
    "icrush-loader-plug": "^0.2.1",
    "icrush-style-loader": "^1.0.3",
    "postcss": "^7.0.21",
    "postcss-loader": "^3.0.0",
    "url-loader": "^2.2.0",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "@yelloxing/normalize.css": "^0.2.1",
    "icrush": "^2.0.1",
    "image2d": "^1.10.6",
    "wscode": "^2.1.0",
    "wscode-prettify": "^0.1.0",
    "xhtml.js": "^1.1.2"
  }
}
