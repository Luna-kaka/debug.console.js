{
  "name": "@yelloxing/debugger",
  "version": "0.1.2",
  "description": "WEB调试工具",
  "main": "build/debug.console.js",
  "scripts": {
    "start": "npm install && npm run build",
    "build": "npm run build:init && npm run build:bundle && npm run build:babel && npm run build:uglifyjs && npm run build:end",
    "build:init": "cuf --config ./cuf.config.js init",
    "build:end": "cuf --config ./cuf.config.js end",
    "build:bundle": "cuf --config ./cuf.config.js bundle",
    "build:babel": "cuf --config ./cuf.config.js babel",
    "build:uglifyjs": "cuf --config ./cuf.config.js uglifyjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yelloxing/debug.console.js.git"
  },
  "keywords": [
    "debugger",
    "console"
  ],
  "author": "心叶",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yelloxing/debug.console.js/issues"
  },
  "homepage": "https://github.com/yelloxing/debug.console.js#readme",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babelify": "^8.0.0",
    "cuf": "^1.1.3",
    "rollup": "^1.17.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "uglify-js": "^3.5.6"
  }
}